    <template id="add_title_hero_image" inherit_id="website.snippets" name="Hero Image Title">
        <xpath expr="//div[@id='snippet_structure']" position="replace"></xpath>
        <xpath expr="//div[@id='snippet_feature']" position="replace"></xpath>
        <xpath expr="//div[@id='snippet_effect']" position="replace"></xpath>
        <xpath expr="//div[@id='snippet_content']" position="replace"></xpath>

        <xpath expr="//div[@id='o_scroll']" position="inside">
            <div id="snippet_hero" class="o_panel">
                <div class="o_panel_header">
                    <i class="fa fa-th-large"/> Hero Image
                </div>
                <div class="o_panel_body">
                    <t t-snippet="theme_vivre.hero_image_one" t-thumbnail="/theme_vivre/static/src/images/banner.png"/>
                </div>
            </div>
        </xpath>

		<xpath expr="//div[@id='snippet_hero']" position="after">
            <div id="snippet_offer" class="o_panel">
                <div class="o_panel_header">
                    <i class="fa fa-th-large"/> Brand Gallery 
                </div>
                <div class="o_panel_body">
                    <t t-snippet="theme_vivre.brand_gallery_one" t-thumbnail="/theme_vivre/static/src/images/banner.png"/>
                </div>
            </div>
        </xpath>

		<xpath expr="//div[@id='snippet_hero']" position="after">
            <div id="snippet_offer" class="o_panel">
                <div class="o_panel_header">
                    <i class="fa fa-th-large"/> Image Gallery 
                </div>
                <div class="o_panel_body">
                    <t t-snippet="theme_vivre.image_gallery_one" t-thumbnail="/theme_vivre/static/src/images/banner.png"/>
                </div>
            </div>
        </xpath>

		<xpath expr="//div[@id='snippet_hero']" position="after">
            <div id="snippet_team_gallery" class="o_panel">
                <div class="o_panel_header">
                    <i class="fa fa-th-large"/> Team Gallery
                </div>
